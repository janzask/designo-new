<section id="{{ project.connect }}" class="properties--locations">
    <img class="properties__image--locations" src="{{ project.image }}" alt="">
  <div class="properties__content--locations">
    <h2 class="properties__title--locations">{{ project.title }}</h2>
    <h3 class="properties__title--service">{{ project.office }}</h3>
    <p class="properties__description--service">{{ project.street }}</p>
    <p class="properties__description--service">{{ project.city }}</p>
    <h3 class="properties__title--service">{{ project.contact }}</h3>
    <p class="properties__description--service">{{ project.phone }}</p>
    <p class="properties__description--service">{{ project.email }}</p>
  </div>
</section>